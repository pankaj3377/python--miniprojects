no1=input("enter your value : ")
no2=input("enter  your 2nd value : ")
no3=int(no1)
no4=int(no2)
operator=input("enter your operator: ")
if operator=="+":
    print("sum of two number",no3+no4)
elif operator=="-":
    print("subtract of two number",no3-no4)

elif operator=="/":
    print("divide of two number",no3/no4)

elif operator=="*":
    print("multiply of two number",no3*no4)
